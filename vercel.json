{
  "framework": "vite",
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "redirects": [
    {
      "source": "/.well-known/farcaster.json",
      "destination": "https://raw.githubusercontent.com/felixripper/cutcut/main/public/.well-known/farcaster.json",
      "permanent": false
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}